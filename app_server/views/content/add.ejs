<div class="row">
    <div class="offset-md-3 col-md-6">
        <h1><%= title %> Developer Contact</h1>
        <form method="POST" class="form">
            <div class="form-group">
                <label for=""> First Name</label>
                <input type="text" class="form-control"
                id="firstNameTextField"
                placeholder="Enter First Name"
                name="firstname" required>
            </div>
            <div class="form-group">
                <label for="">Last Name</label>
                <input type="text" class="form-control"
                id="lastNameTextField"
                placeholder="Enter Last Name"
                name="lastname" required>
            </div>
            <div class="form-group">
                <label for="">Field</label>
                <input type="text" class="form-control"
                id="fieldTextField"
                placeholder="Enter Field"
                name="field" required>
            </div>
            <div class="form-group">
                <label for="">Ethnicity</label>
                <textarea type="text" class="form-control"
                id="ethnicityTextArea"
                placeholder="Enter Ethnicity"
                name="ethnicity" required></textarea>
            </div>
            <div class="form-group">
                <label for="">Century</label>
                <input type="number" class="form-control"
                id="centuryTextField"
                placeholder="Enter Century"
                name="century" required>
            </div>
            <button type="submit" class="btn btn-primary">
                <i class="fas fa-edit"></i> Submit
            </button>
            <a href="/contacts" class="btn btn-warning">
                <i class="fas fa-undo"></i> Cancel</a>
        </form>
    </div>
</div>
